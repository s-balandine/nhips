* Q404.1 | HIV Test Consent (HEW_HIV_TEST_CONSENT) | NUMERIC(1)
* Consent
* Allowed Values
*    1: Granted
*    2: Refused

Q4041
  MUSTENTER
  COMMENT LEGAL USE D_N_CONSENT
  TYPE COMMENT LEGAL
  BEFORE ENTRY
    IF ( HEW_BIRTH_YEAR <> 98) THEN
      LET AGE = HEW_BIRTH_YEAR
      IF ( HEW_BIRTH_MONTH <> 98) THEN
        LET AGE = AGE + ( HEW_BIRTH_MONTH / 12)
      ENDIF
    ELSE
      IF ( HEW_BIRTH_MONTH <> 98) THEN
        LET AGE = HEW_BIRTH_MONTH / 12
      ELSE
        LET AGE = 98
      ENDIF
    ENDIF
    IF (AGE>=18) AND (AGE<=49) THEN
      GOTO Q4061
    ENDIF
    IF ( HEW_MARITAL_STATUS <> 4 THEN
      GOTO Q4061
    ENDIF
  END
  AFTER ENTRY
    IF Q4041 =2 THEN
      GOTO WRITEREC
    ENDIF
  END
  END

